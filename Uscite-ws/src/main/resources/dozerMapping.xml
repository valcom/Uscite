<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<configuration>
		<wildcard>true</wildcard>
	</configuration>
	<mapping type="one-way">
		<class-a >org.springframework.data.domain.Page
		</class-a>
		<class-b>it.ccse.uscite.application.facade.dto.output.PageDTO
		</class-b>
		<field >
			<a get-method="getNumber">number</a>
			<b>number</b>
		</field>
		<field>
			<a get-method="getNumberOfElements">numberOfElements</a>
			<b>numberOfElements</b>
		</field>
		<field>
			<a get-method="getSize">size</a>
			<b>size</b>
		</field>
		<field>
			<a get-method="getTotalPages">totalPages</a>
			<b>totalPages</b>
		</field>
		<field>
			<a get-method="getTotalElements">totalElements</a>
			<b>totalElements</b>
		</field>
		<field>
			<a get-method="getContent">content</a>
			<b set-method="setContent">content</b>
		</field>

	</mapping>
</mappings>
